{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", "jsx", "ts", "tsx"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_hooks": "./src/hooks",
          "_utils/*": "./src/utils",
          "_styles": "./src/styles",
          "_locales/*": "./src/locales",
          "_types/*": "./src/types"
        }
      }
    ],
    "@babel/plugin-proposal-export-namespace-from"
  ]
}
