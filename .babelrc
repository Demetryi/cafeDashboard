{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_navigations": "./src/navigations",
          "_scenes": "./src/scenes",
          "_services": "./src/services",
          "_styles": "./src/styles",
          "_utils": "./src/utils",
          "_store": "./src/store",
          "_localization": "./src/localization"
        }
      }
    ],
    "@babel/plugin-proposal-export-namespace-from"
  ]
}
